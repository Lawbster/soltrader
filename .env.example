# Helius RPC
HELIUS_API_KEY=your_helius_api_key_here
HELIUS_RPC_URL=https://mainnet.helius-rpc.com/?api-key=YOUR_KEY
HELIUS_WSS_URL=wss://mainnet.helius-rpc.com/?api-key=YOUR_KEY

# Wallet (NEVER commit your real private key)
WALLET_PRIVATE_KEY=your_base58_private_key_here

# Trading config
PAPER_TRADING=true
MAX_POSITION_SOL=1.0
MAX_CONCURRENT_POSITIONS=5
DEFAULT_SLIPPAGE_BPS=300

# Universe selection
# launches = pump.fun + Raydium new pools (default)
# watchlist = only mints from config/watchlist.json or WATCHLIST_MINTS
# both = launches + watchlist
UNIVERSE_MODE=launches
WATCHLIST_MINTS=

# Dashboard (optional, default 3847)
DASHBOARD_PORT=3847

# Alerts (optional)
DISCORD_WEBHOOK_URL=
TELEGRAM_BOT_TOKEN=
TELEGRAM_CHAT_ID=
